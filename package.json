    "migrate:supabase": "node migrate-from-supabase.js",
    "vercel-build": "node scripts/check-deployment-env.js && npm run deploy && npm run build",
    "railway-build": "npm ci --omit=dev --prefer-offline && npx prisma generate && npx next build",
    "netlify-build": "node scripts/check-deployment-env.js && prisma generate && next build",
    "postbuild": "echo 'Build completed successfully!'",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true npm run build"
