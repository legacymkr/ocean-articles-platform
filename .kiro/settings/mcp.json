{
  "mcpServers": {
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp@latest"
      ],
      "autoApprove": [
        "new_page",
        "take_snapshot",
        "click",
        "navigate_page",
        "list_network_requests",
        "get_network_request",
        "list_console_messages",
        "upload_file"
      ]
    }
  }
}